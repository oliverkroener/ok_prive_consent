{
    "name": "oliverkroener/ok-prive-consent",
    "type": "typo3-cms-extension",
    "description": "Provides a backend module to manage privacy cookie consent scripts for Prive Cookie Consent.",
    "homepage": "https://www.oliver-kroener.de",
    "version": "3.1.1",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "typo3",
        "typo3-extension",
        "cookie-consent",
        "privacy",
        "gdpr",
        "prive",
        "consent-management",
        "backend-module"
    ],
    "authors": [
        {
            "name": "Oliver Kroener",
            "email": "ok@oliver-kroener.de",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "OliverKroener\\OkPriveConsent\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ok_prive_consent"
        }
    }
}
